<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fh.dao.dept.IDeptDao" >
	

<resultMap type="com.fh.bean.dept.DeptBean" id="BaseDeptMap">
		<id column="cdeptid" property="cdeptid" jdbcType="VARCHAR" />
		<result column="cdeptname" property="cdeptname" jdbcType="VARCHAR" />
		<result column="cpid" property="cpid" jdbcType="VARCHAR" />
		<result column="nvalid" property="nvalid" jdbcType="INTEGER" />
	</resultMap>
	
	<select id="queryDeptZtree" parameterType="com.fh.bean.dept.DeptBean" resultMap="BaseDeptMap">
	
		select cdeptid,cpid,cdeptname from t_fh_dept 
		
	</select>
	
	<insert id="addDept">
		insert INTO t_fh_dept (cdeptid,cpid,cdeptname,nvalid) 
		
		values
		<foreach collection="list"  item="dept"  index="index" separator=",">
		
		(REPLACE(UUID(),'-',''),#{dept.cpid,jdbcType=VARCHAR},#{dept.cdeptname,jdbcType=VARCHAR},#{dept.nvalid,jdbcType=INTEGER}) 
		</foreach>
		
		
	</insert>
	<delete id="deleteDept">

			delete from t_fh_dept where  cdeptid   in 
			
			<foreach collection="array"  item="id"  index="index"  open="(" close=")" separator=",">
					#{id,jdbcType=VARCHAR}
			</foreach>

	</delete>
	
	<update id="updateDept" parameterType="com.fh.bean.dept.DeptBean">
	
		update 	t_fh_dept set cdeptname = #{cdeptname} where cdeptid = #{cdeptid}
	
	</update>
	
	
	
	<select id="querydept" resultMap="BaseDeptMap">
		select cdeptid,cdeptname from t_fh_dept
	
	</select>
	
	
	
	
	
